    </div>

    <div class="app-page" id="root"></div>
    <div class="cds--css-grid">
      <div
        class="cds--sm:col-span-4 cds--md:col-span-3 cds--lg:col-span-5 cds--css-grid-column"
      >
        <div class="cds--data-table-container">
          <div class="cds--data-table-header">
            <h4 class="cds--data-table-header__title" id="tc-:r1h:-title">
              latest observations
            </h4>
            <p
              class="cds--data-table-header__description"
              id="tc-:r1h:-description"
            />
          </div>
          <div class="cds--data-table-content">
            <table
              aria-labelledby="tc-:r1h:-title"
              class="cds--data-table cds--data-table--xs"
            >
              <tbody aria-live="polite">
                <tr>
                  <td>Barometer</td>
                  <td>$current.barometer</td>
                </tr>
                <tr>
                  <td>Visibility</td>
                  <td>$current.visibility</td>
                </tr>
                <tr>
                  <td>Ceiling</td>
                  <td>$current.ceiling</td>
                </tr>
                <tr>
                  <td>Dewpoint</td>
                  <td>$current.dewpoint &#176;F</td>
                </tr>
                <tr>
                  <td>Humidity</td>
                  <td>$current.outHumidity</td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div
        class="cds--sm:col-span-4 cds--md:col-span-4 cds--lg:col-span-6 cds--css-grid-column"
      >
        <div class="cds--data-table-container">
          <div class="cds--data-table-content">
            <table
              aria-labelledby="tc-:r1h:-title"
              class="cds--data-table cds--data-table--xs"
            >
              <tbody aria-live="polite">
                <tr>
                  <td>
                    <p class="label">$gettext("Sun")</p>
                  </td>
                </tr>
                <tr class="stats">
                  <td>
                    <p class="label small">$gettext("Start civil twilight")</p>
                    <p class="value">
                      $almanac(horizon=-6).sun(use_center=1).rise
                    </p>
                  </td>
                  <td>
                    <p class="label small">$gettext("End civil twilight")</p>
                    <p class="value">
                      $almanac(horizon=-6).sun(use_center=1).set
                    </p>
                  </td>
                </tr>
                <tr class="stats">
                  <td>
                    <p class="label small">$gettext("Start nautic twilight")</p>
                    <p class="value">
                      $almanac(horizon=-12).sun(use_center=1).rise
                    </p>
                  </td>
                  <td>
                    <p class="label small">$gettext("End nautic twilight")</p>
                    <p class="value">
                      $almanac(horizon=-12).sun(use_center=1).set
                    </p>
                  </td>
                </tr>
                <tr class="stats">
                  <td>
                    <p class="label small">
                      $gettext("Start astronomic twilight")
                    </p>
                    <p class="value">
                      $almanac(horizon=-18).sun(use_center=1).rise
                    </p>
                  </td>
                  <td>
                    <p class="label small">
                      $gettext("End astronomic twilight")
                    </p>
                    <p class="value">
                      $almanac(horizon=-18).sun(use_center=1).set
                    </p>
                  </td>
                </tr>
                <tr class="stats">
                  <td>
                    <p class="label small">$gettext("Azimuth")</p>
                    <p class="value">$("%.1f&deg;" % $almanac.sun.az)s</p>
                  </td>
                  <td>
                    <p class="label small">
                      $pgettext("Astronomical", "Altitude")
                    </p>
                    <p class="value">$("%.1f&deg;" % $almanac.sun.alt)</p>
                  </td>
                </tr>
                <tr class="stats">
                  <td>
                    <p class="label small">$gettext("Right ascension")</p>
                    <p class="value">$("%.1f&deg;" % $almanac.sun.ra)</p>
                  </td>
                  <td>
                    <p class="label small">$gettext("Declination")</p>
                    <p class="value">$("%.1f&deg;" % $almanac.sun.dec)</p>
                  </td>
                </tr>
                <!--prettier-ignore-->
                #if $almanac.next_equinox.raw < $almanac.next_solstice.raw
                    ## The equinox is before the solstice. Display them in order.
                <tr class="stats">
                  <td>
                    <p class="label small">$gettext("Equinox")</p>
                    <p class="value">$almanac.next_equinox</p>
                  </td>
                  <td>
                    <p class="label small">$gettext("Solstice")</p>
                    <p class="value">$almanac.next_solstice</p>
                  </td>
                </tr>
                <!--prettier-ignore-->
                #else
                    ## The solstice is before the equinox. Display them in order.
                <tr class="stats">
                  <td>
                    <p class="label small">$gettext("Solstice")</p>
                    <p class="value">$almanac.next_solstice</p>
                  </td>
                  <td>
                    <p class="label small">$gettext("Equinox")</p>
                    <p class="value">$almanac.next_equinox</p>
                  </td>
                </tr>
                #end if
                <tr>
                  <td>
                    <p class="label small">$gettext("Total daylight")</p>
                    <p class="value">$almanac.sun.visible.long_form</p>
                    <p class="label small">
                      <!--prettier-ignore-->
                      $almanac.sun.visible_change.long_form
                          #if $almanac.sun.visible_change.raw < 0
                            $gettext("less than yesterday")
                          #else
                            $gettext("more than yesterday")
                          #end if
                    </p>
                  </td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <div>
      <!--prettier-ignore-->
      #if $almanac.hasExtras
      <div>
        <h2>$gettext("Last 24 hours")</h2>
        <div>$("%.1f&deg;" % $almanac.sun.alt)</div>
        <div class="value-col">
          <p class="label small">$gettext("Sunrise")</p>
          <p class="value">$almanac.sunrise</p>
        </div>
      </div>
      #end if
    </div>

    <div class="oklahoma"></div>
  </body>
</html>
